<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

	<!-- Occupied -->
	<item android:color="@color/white" android:state_activated="true" android:state_selected="false"/>

	<!-- Billed -->
	<item android:color="@color/white" android:state_activated="true" android:state_selected="true"/>

	<!-- Reserved -->
	<item android:color="@color/white" android:state_checkable="false" android:state_selected="true"/>

	<!-- Normal -->
	<item android:color="@color/black"/>

</selector>